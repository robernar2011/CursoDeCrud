<form>
    <fieldset>
        <!-- Text input-->
        <div class="form-row">
            <span class="col-md-4"></span>
            <div class="col-md-1">
                <label class="control-label" for="txtNome">Nome</label>
            </div>
            <div class="col-md-3">
                <input id="txtNome" required maxlength="26" name="txtNome" type="text" placeholder="" class="form-control input-md">
            </div>
        </div>
        <br />
        <!-- Text input-->
        <div class="form-row">
            <div class="col-md-4"></div>
            <div class="col-md-1">
                <label class="control-label" for="txtCpf">CPF</label>
            </div>
            <div class="col-md-2">
                <input id="txtCpf" required maxlength="14" name="txtCpf" type="text" placeholder="_ _ _._ _ _._ _ _-_ _" class="cpf form-control input-md">
            </div>
        </div>
        <br />

        <!-- Select Basic -->
        <div class="form-row">
            <div class="col-md-4"></div>
            <div class="col-md-1">
                <label class="control-label" for="comboUF">UF</label>
            </div>
            <div class="col-md-2">
                <select id="comboUF" required class="form-control"
                        onchange="ListarCidades()">
                    <option value="0" selected="selected">Selecione</option>
                </select>
            </div>
        </div>
        <br />

        <!-- Select Basic -->
        <div class="form-row">
            <div class="col-md-4"></div>
            <div class="col-md-1">
                <label class="control-label" for="comboCidade">Cidades</label>
            </div>
            <div class="col-md-2">
                <select id="comboCidade" required class="form-control">
                </select>
            </div>
        </div>
        <br />
        <!-- Button (Double) -->
        <div class="form-row">
            <div class="col-md-4"></div>
            <label class="control-label" for="button1id"></label>
            <div class="col-md-8">
                <div id="btnSalvar" style="cursor:pointer" name="btnSalvar" class="btn btn-success" onclick="Salvar()">Salvar</div>
                <div id="btnclean" style="cursor:pointer" name="btnclean" class="btn btn-danger" onclick="cancel()">Cancelar</div>
            </div>
        </div>
    </fieldset>
</form>
<br />
<br />
